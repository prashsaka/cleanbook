{
  "name": "cleanbook",
  "description": "Cleans up Facebook page by removing unwanted posts/ads.",
  "version": "0.0.1",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "content_scripts": [{
    "matches": [
      "https://www.facebook.com/*",
      "https://facebook.com/*"
    ],
    "js": ["content-script.js"]
  }],
  "manifest_version": 2
}
